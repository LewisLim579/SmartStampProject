<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >
    
    <LinearLayout
        android:id="@+id/ll_top_branch" 
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/ll_top_1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_weight="5"
        android:background="#111111"
        android:gravity="bottom"
        android:orientation="vertical"
        android:padding="10dp" >

        <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:gravity="center_vertical" >

            <ImageView
                android:id="@+id/roundlogo1"
                android:layout_width="70dp"
                android:layout_height="70dp"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="15dp"
                 />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_favor_1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#FFFFFFFF"
                    android:textSize="18dp" />

                <TextView
                    android:id="@+id/tv_rank_count1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    
                    android:layout_marginTop="5dp"
                    android:textColor="#FFFFFFFF"
                    android:layout_marginLeft="5dp"
                    android:textSize="15dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center|right"
                 android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|right"
                    android:layout_marginRight="10dp"
                    android:gravity="center"
                    android:text="1"
                    android:textColor="#FFFFFFFF"
                    android:textSize="60dp" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:padding="10dp"
            android:paddingLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/storkxml"
            android:gravity="center_vertical" >

            <ImageView
                android:id="@+id/roundlogo2"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                 />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_favor_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#E6696969"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tv_rank_count2"
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    
                    android:layout_marginTop="5dp"
                    android:textColor="#E6696969"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center|right"
                 android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|right"
                    android:gravity="center"
                    android:text="2"
                    android:layout_marginRight="15dp"
                    android:textColor="#E6696969"
                    android:textSize="40dp" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:padding="10dp"
            android:paddingLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/storkxml"
            android:gravity="center_vertical" >

            <ImageView
                android:id="@+id/roundlogo3"
                android:layout_width="60dp"
                 android:layout_marginLeft="10dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"
                 />

            <LinearLayout
                android:layout_width="wrap_content"
                android:gravity="center_vertical"
                android:layout_height="match_parent"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_favor_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#C8696969"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tv_rank_count3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#C8696969"
                    
                    android:layout_marginTop="5dp"
                    android:layout_marginLeft="5dp"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center|right"
                 android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|right"
                    android:gravity="center"
                    android:text="3"
                    android:layout_marginRight="15dp"
                    android:textColor="#C8696969"
                    android:textSize="40dp" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:padding="10dp"
            android:paddingLeft="10dp"
            android:background="@drawable/storkxml"
            android:layout_weight="1"
            android:gravity="center_vertical" >

            <ImageView
                android:id="@+id/roundlogo4"
                android:layout_width="60dp"
                 android:layout_marginLeft="10dp"
                android:layout_height="60dp"
                android:layout_marginRight="10dp"
                 />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_favor_4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#AA696969"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tv_rank_count4"
                    android:layout_marginLeft="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    
                    android:layout_marginTop="5dp"
                    android:textColor="#AA696969"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center|right"
                 android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|right"
                    android:gravity="center"
                    android:text="4"
                    android:textColor="#AA696969"
                    android:layout_marginRight="15dp"
                    android:textSize="40dp" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:id="@+id/LinearLayout1"
            android:layout_width="fill_parent"
            android:layout_height="70dp"
            android:padding="10dp"
            android:paddingLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/storkxml"
            android:gravity="center_vertical" >

            <ImageView
                android:id="@+id/roundlogo5"
                android:layout_width="60dp"
                android:layout_height="60dp"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                 />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical" >

                <TextView
                    android:id="@+id/tv_favor_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="#8C696969"
                    android:textSize="15dp" />

                <TextView
                    android:id="@+id/tv_rank_count5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:textColor="#8C696969"
                    android:textSize="12dp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center|right"
                 android:gravity="center"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|right"
                    android:gravity="center"
                    android:text="5"
                    android:layout_marginRight="15dp"
                    android:textColor="#8C696969"
                    android:textSize="40dp" />
            </LinearLayout>
        </LinearLayout>

    
</LinearLayout>
<RelativeLayout
    android:background="@drawable/back_logo"
        android:id="@+id/example_rl"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/TextView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="방문한 매장이 없습니다." />

    </RelativeLayout>

</FrameLayout>